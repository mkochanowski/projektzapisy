{
  "extends": "tslint-config-standard",
  "rules": {
    "semicolon": [true, "always"],
    "quotemark": [true, "double"],
    "space-before-function-paren": false,
    "indent": [true, "spaces", 4],
    "ter-indent": [
      true, 4,
      {
        "SwitchCase": 1
      }
    ],
    "new-parens": false,
    "max-line-length": [true, 80],
    "trailing-comma": false,
    "variable-name": [
      true,
      "check-format"
    ]
  }
}
